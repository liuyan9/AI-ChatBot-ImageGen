<head>
  <style>
    #loading-icon {
  display: none;
  position: relative;
  width: 64px;
  height: 64px;
  margin: 0 auto;
}

#loading-icon:before,
#loading-icon:after {
  content: "";
  display: block;
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  border-radius: 50%;
  border: 3px solid transparent;
  animation: spin 1s linear infinite;
}

#loading-icon:before {
  border-top-color: #333;
  border-bottom-color: #333;
  animation-delay: 0.5s;
}

#loading-icon:after {
  border-left-color: #333;
  border-right-color: #333;
}

@keyframes spin {
  0% {
    transform: rotate(0deg);
  }
  100% {
    transform: rotate(360deg);
  }
}

    body {
  font-family: Arial, sans-serif;
  font-size: 16px;
  color: #333;
  padding: 1rem;
}

h2 {
  margin-bottom: 1rem;
}

#image-form {
  margin-bottom: 1rem;
}

#image-form label {
  display: block;
  margin-bottom: 0.5rem;
}

#image-form input[type="text"] {
  width: 100%;
  padding: 0.5rem;
  font-size: 16px;
  margin-bottom: 1rem;
}

#image-form button[type="submit"] {
  padding: 0.5rem 1rem;
  background-color: #333;
  color: #fff;
  border: 0;
  border-radius: 4px;
  font-size: 16px;
  cursor: pointer;
}

#image-container {
  margin-top: 1rem;
  text-align: center;
}

#image-container img {
  max-width: 100%;
  height: auto;
}

